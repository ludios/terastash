{
  "name": "intreader",
  "version": "1.0.1",
  "description": "Streamable uint32-prefixed frame decoder for node.  Supports both little-endian and big-endian sizes.  Size can include or exclude the 4 bytes of length itself.  No O(N^2) problems on data trickling in.  No dependencies.",
  "homepage": "https://github.com/ludios/intreader",
  "issues": "https://github.com/ludios/intreader/issues",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/.bin/_mocha tests/*.js && ./node_modules/.bin/eslint .",
    "test-strong-mode": "node --strong-mode ./node_modules/.bin/_mocha tests/*.js"
  },
  "keywords": [
    "decode",
    "decoder",
    "frame",
    "binary"
  ],
  "author": "Ivan Kozik",
  "license": "ISC",
  "repository": "ludios/intreader",
  "devDependencies": {
    "ayy": "^0.2.4",
    "better-buffer-inspect": "^0.1.4",
    "bluebird": "^3.0.0",
    "eslint": "^1.7.3",
    "mocha": "^2.3.3"
  },
  "engines": {
    "iojs": ">= 2.0.0",
    "node": ">= 4.0.0"
  }
}
